{
  "name": "Play with fshare",
  "version": "1.4.2",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "18": "icon.png",
      "19": "icon.png"
    }
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["nativeMessaging", "*://*.fshare.vn/*", "contextMenus", "webRequest", "webRequestBlocking", "cookies"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
