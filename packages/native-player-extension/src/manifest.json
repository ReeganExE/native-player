{
  "name": "Play with fshare",
  "description": "Open URL with your favourite media player",
  "version": "1.6.1",
  "background": {
    "scripts": [
      "vendor.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "18": "icon.png",
      "19": "icon.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "nativeMessaging",
    "https://*.fshare.vn/*",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "cookies"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}